{
  "name": "comarch-b2b",
  "version": "2020.2.0",
  "scripts": {
    "ng": "ng",
    "svg-sprite": "svg-sprite --config svg-sprite-config.json assets/images/svg/*.svg",
    "start": "ng serve --serve-path=/ --deployUrl=/",
    "build-ngsw": "node_modules/.bin/ngsw-config ./ ./ngsw-config.json",
    "workers-copy": "move /Y prod\\worker-basic.min.js .. && move /Y prod\\safety-worker.js ..",
    "redundant-ngsw-remove": "del prod\\ngsw.json && del ngsw.json",
    "ngsw-copy": "move /Y prod\\worker-basic.min.js .. && move /Y prod\\safety-worker.js .. && move /Y prod\\ngsw.json ..",
    "build": "ng build --output-path=prod/ --deployUrl=/ClientApp/prod/ --prod --extra-webpack-config webpack.partial.js",
    "prod-build": "npm run svg-sprite && npm run build-ngsw && npm run build && npm run workers-copy && npm run redundant-ngsw-remove",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
